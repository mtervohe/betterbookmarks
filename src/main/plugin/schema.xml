<schema name="betterbookmark">

    <!--  Table for Betterbookmark -->
    <table name="managedBookmark" description="Managed Bookmark list">
        <column name="productID" type="bigint" nullable="false" description="Product ID"/>
        <column name="productName" type="varchar" size="255" nullable="false" unicode="true"
                description="Display name of the product"/>

        <index type="primary" name="supProduct_pk" column="productID"/>
    </table>

</schema>