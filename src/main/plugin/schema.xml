<schema name="betterbookmark">

    <!--  Table for managedBookmark -->
    <table name="managedBookmark" description="Managed Bookmark list">
        <column name="bookmarkID" type="bigint" nullable="false" description="Product ID"/>
        <column name="bookmarkName" type="varchar" size="255" nullable="false" unicode="true"
                description="Display name of the bookmark"/>

        <index type="primary" name="manBookmark_pk" column="bookmarkID"/>
    </table>

</schema>